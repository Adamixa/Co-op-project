@model IEnumerable<UniversityResturantInformation.Models.Item>

@{
    ViewData["Title"] = "Rating Result";
    Layout = "AdminLayout";
}

<br />
<br />
<br />
<br />
<div class="table-responsive">
    <table id="myTable" class="table table-striped table-sm border">
        <thead>
            <tr>
                <th></th>
                <th></th>
                <th>
                     Item Name
                </th>
                <th>
                    Rating
                </th>
                <th>
                   Number of ratings
                </th>
                
            </tr>
        </thead>
        <tbody>
            @foreach (var item in Model)
            {
                <tr>
                    <td></td>
                    <td></td>
                    <td>
                        @Html.DisplayFor(modelItem => item.ItemName )
                    </td>

                    <td>
                        @Html.DisplayFor(modelItem => item.Total)/5
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.NumberOfRating)
                    </td>
                    
                </tr>
            }
        </tbody>
    </table>
</div>